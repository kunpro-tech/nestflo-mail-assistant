{
  "name": "kunpro-mail-assistant",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "kunpro-mail-assistant",
  "author": "kunpro-tech",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/kunpro-tech/kunpro-mail-assistant"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test",
    "start": "electron-forge start",
    "package": "vite build && electron-forge package",
    "make": "vite build && electron-forge make",
    "make-mac": "vite build && electron-forge make --platform=darwin --arch=arm64 && electron-forge make --platform=darwin --arch=x64",
    "make-mac-dev": "vite build --mode development && electron-forge make --platform=darwin --arch=arm64 && electron-forge make --platform=darwin --arch=x64",
    "make-window": "vite build && electron-forge make",
    "make-window-dev": "vite build --mode development && electron-forge make"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@electron-forge/maker-wix": "^6.1.1",
    "antd": "^5.5.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "graphql": "^16.6.0",
    "imap": "^0.8.19",
    "imapflow": "^1.0.128",
    "mailparser": "^3.6.4",
    "nodemailer": "^6.9.3",
    "react-router-dom": "^6.11.2",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-dmg": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/publisher-github": "^6.1.1",
    "@playwright/test": "^1.34.3",
    "@types/imap": "^0.8.37",
    "@types/imapflow": "^1.0.13",
    "@types/mailparser": "^3.4.0",
    "@types/nodemailer": "^6.4.8",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^23.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "typescript": "^5.1.3",
    "vite": "^4.1.5",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  }
}
